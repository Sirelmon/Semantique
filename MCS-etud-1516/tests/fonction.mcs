int somme (int a, int b) {
	return a+b;
}

int somme (int a, int b, int c) {
	return a+b+c;
}

int somme (char a, int c) {
	return c;
}

int fact (int hey) {
	if (hey == 0) {
		return 1;
	} else {
		return hey*fact(hey-1);
	}
}		

